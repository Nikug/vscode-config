// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+n",
    "command": "goToNextReferenceFromEmbeddedEditor",
    "when": "vim.active && (inReferenceSearchEditor || referenceSearchVisible)"
  },
  {
    "key": "ctrl+p",
    "command": "goToPreviousReferenceFromEmbeddedEditor",
    "when": "vim.active && (inReferenceSearchEditor || referenceSearchVisible)"
  },
  {
    "key": "ctrl+n",
    "command": "selectNextCodeAction",
    "when": "vim.active && codeActionMenuVisible"
  },
  {
    "key": "ctrl+p",
    "command": "selectPrevCodeAction",
    "when": "vim.active && codeActionMenuVisible"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "vim.active && inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "vim.active && inQuickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.files.newUntitledFile",
    "when": "!vim.active"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen",
    "when": "!vim.active"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpen"
  },
]